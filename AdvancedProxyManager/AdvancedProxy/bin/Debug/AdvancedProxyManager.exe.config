<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="ProxyDefinition" type="Fleischmann.AdvancedProxy.ProxyConfiguration.ProxyDefinitionSection, AdvancedProxyManager" />
	</configSections>

	<ProxyDefinition>
  <ProxyDefinitions>
   <Proxy Name="Initial Proxy Settings" AutoDetect="false" UseAutoConfigScript="false"
    AutoConfigScriptAddress="" UseProxyServer="false" UseProxyServerAddress=""
    UseProxyServerPort="" UseSameProxyServerForAll="false" BypassProxyForLocalAddress="true"
    ExcludeAddressesFromProxy="*,eds.com;*.eds.com:8080;127.0.0.1">
    <ProxyURLs>
     <add Protocol="HTTP" URL="HTTP" Port="12" />
     <add Protocol="HTTPS" URL="Secure" Port="123" />
     <add Protocol="FTP" URL="FTP" Port="393" />
     <add Protocol="Gopher" URL="Gopher" Port="333" />
    </ProxyURLs>
   </Proxy>
  </ProxyDefinitions>
 </ProxyDefinition>
</configuration>